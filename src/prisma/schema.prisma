generator client {
  provider = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model PostMeta {
  id        string   @id @default(dbgenerated()) @map("_id") @db.ObjectId
  title     string
  createdAt DateTime @default(now())
  slug      string   @unique
  likes     Int      @default(1)
  views     Int      @default(0)
}
